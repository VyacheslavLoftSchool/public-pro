-var img = [{'id': '1', 'img': 'w1.png'},{'id': '2', 'img': 'w2.png'},{'id': '3', 'img': 'w3.png'},{'id': '4', 'img': 'w4.png'}];
ul.works__list
    each item in img
        li.works__list-item
            img.works__list-img(src='./src/images/content/'+item.img, alt='')
                    
ul.numbers-list
    li.numbers-list(style='display:none;') #{img[0].id}
    li.numbers-list(style='display:none;') #{img[1].id}
    li.numbers-list(style='display:none;') #{img[2].id}
    li.numbers-list #{img[3].id}